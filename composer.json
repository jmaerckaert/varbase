            },
            "drupal/linkit": {
                "Issue #3078075: Detect and strip base URL from pasted URLs to increase matching hits and support multilingual": "https://www.drupal.org/files/issues/2021-01-14/3078075-22.patch"
            },
            "drupal/pathologic": {
                "Issue #2418369: Internal URL handling (Aliases, language prefixes, base://, ...)": "https://www.drupal.org/files/issues/2022-11-25/pathologic-2418369-46_0.patch"
            },
            "drupal/token": {
