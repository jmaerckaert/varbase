            },
            "drupal/ctools": {
                "Issue #2962166: Rendering entity view block on an entity with the same view mode causes recursion infinite loop": "https://www.drupal.org/files/issues/2020-08-19/ctools-Rendering_entity_view_block_on_an_entity_with_the_same_view_mode_causes_recursion_infinite_loop-2962166-5.patch"
            }
        }
    }
